/* components/search/search.wxss */
@import 'incofont.wxss';
.container {
    display: flex;
    align-items: center;
    margin-left: 20rpx;
    position: relative;
}

.bar {
    /* width: 180%; */
    height: 68rpx;
    font-size: 28rpx;
    border-radius: 50px;
    background-color: #f5f5f5;
    padding-left: 60rpx;
}
.search {
    background-color: #d43c33;
    color: #fff;
    margin-left: 20rpx;
    font-size: 24rpx;
}
.inBar {
    color: #999;
}
.iconfont {
    position: absolute;
    left: 20rpx;
    font-size: 36rpx;
}
.iconfont .icon-sousuotianchong {
    position: absolute;
    top: 50%;
    transform: translate(16rpx,-50%);
    color: #9f9f9f;
}